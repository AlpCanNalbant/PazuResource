{
    "configurations": [
        {
            "name": "C/C++ GDB: Launch Pazu Resource Generator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/ResourceGenerator.exe",
            "args": [
                "Resource/Res.txt",
                "${workspaceFolder}/bin/Debug/data/Res.txt",
                "Resource/Res2.txt",
                "${workspaceFolder}/bin/Debug/data/Res2.txt"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++ GCC: Debug Build Pazu Resource"
        }
    ]
}
